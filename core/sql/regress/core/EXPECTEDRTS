>>
>>create table tstat (a char(5) not null ,
+>                     b smallint not null ,
+>                     c char(4),
+>                     d integer,
+>                     primary key (a,b) )
+>                     salt using 2 partitions ;

--- SQL operation complete.
>>insert into tstat values ('AAAA ',11,'AAAA',11);

